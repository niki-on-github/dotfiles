" Disable default mappings
let g:EasyMotion_do_mapping = 0
" Turn on case-insensitive feature
let g:EasyMotion_smartcase = 1

map <Localleader><Localleader>. <Plug>(easymotion-repeat)
map <Localleader><Localleader>s <Plug>(easymotion-overwin-f2)
map <Localleader><Localleader>j <Plug>(easymotion-j)
map <Localleader><Localleader>k <Plug>(easymotion-k)
map <Localleader><Localleader>f <Plug>(easymotion-overwin-f)
map <Localleader><Localleader>w <Plug>(easymotion-overwin-w)
